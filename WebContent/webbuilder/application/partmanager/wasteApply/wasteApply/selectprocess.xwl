<?xml version="1.0" encoding="utf-8"?>
<body name="body" class="sys_normal" createBody="false">
	<dataProvider name="dataProvider" metaData="-" sql="select * from(&#xA;select task.proc_inst_id_ procinstid,&#xA;task.ID_ taskid,&#xA;long_ status,&#xA;proc.START_USER_ID_ startname,&#xA;task.name_ taskname ,&#xA;comm.MESSAGE_ remark,&#xA;task.assignee_ checkman,&#xA;task.start_time_ begintime,&#xA;task.end_time_ endtime,&#xA;round(ifnull(task.duration_,0)/(60*60*1000),2) as duration,&#xA;proc.business_key_ bussinesskey ,&#xA;proc.start_time_ starttime  from act_hi_varinst ahv&#xA;join act_hi_taskinst task on ahv.task_id_=task.id_ and ahv.NAME_=task.TASK_DEF_KEY_ join act_hi_procinst proc on proc.id_=task.proc_inst_id_&#xA;left join  act_hi_comment comm on comm.TASK_ID_=task.ID_&#xA;) process&#xA;{#whereSql#}&#xA;order by begintime asc" totalSql="select count(1) from(&#xA;SELECT&#xA;task.id_ id,task.proc_inst_id_ procinstid&#xA;FROM act_hi_varinst ahv&#xA;join act_hi_taskinst task on ahv.task_id_=task.id_ and ahv.NAME_=task.TASK_DEF_KEY_ join act_hi_procinst proc on proc.id_=task.proc_inst_id_&#xA;left join  act_hi_comment comm on comm.TASK_ID_=task.ID_&#xA;)process {#whereSql#}"/></body>
