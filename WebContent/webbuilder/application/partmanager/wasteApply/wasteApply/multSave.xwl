<?xml version="1.0" encoding="utf-8"?>
<body name="body" class="sys_normal" compact="true" simpleError="true" createBody="false">
	<dbUpdater name="dbUpdater" data="{#detailgrid#}" insertSql="insert  into t_waste_apply_detail ( &#xA;PUR_NO,PARTS_COD,C_PARTS_NAM,E_PARTS_NAM,NEW_VALUE,OLD_VALUE,DISCOUNT_YEAR,&#xA;STAND_TXT,PUR_TXT,PUR_NUM,MIN_UNIT,PUR_WAY_COD,REMARKS,&#xA;RECORD_TIM,&#xA;RECORD_MAN,&#xA;DEPT_COD,&#xA;USERID,USE_DATE&#xA;) values(&#xA;{?PUR_NO?},{?PARTS_COD?},{?C_PARTS_NAM?},{?E_PARTS_NAM?},{?NEW_VALUE?},&#xA;{?OLD_VALUE?},{?DISCOUNT_YEAR?},&#xA;{?STAND_TXT?},{?PUR_TXT?},&#xA;{?PUR_NUM?},{?MIN_UNIT?},{?PUR_WAY_COD?},{?REMARKS?},&#xA;now(),&#xA;'{#sys.username#}',&#xA;'{#sys.deptCode#}',&#xA;'{#sys.userid#}',{?USE_DATE?}&#xA;)" updateSql="UPDATE  t_waste_apply_detail SET PARTS_COD={?PARTS_COD?},&#xA;C_PARTS_NAM={?C_PARTS_NAM?},&#xA;E_PARTS_NAM={?E_PARTS_NAM?},&#xA;STAND_TXT={?STAND_TXT?},&#xA;PUR_TXT={?PUR_TXT?},&#xA;PUR_NUM={?PUR_NUM?},&#xA;PUR_WAY_COD={?PUR_WAY_COD?},&#xA;REMARKS={?REMARKS?},USE_DATE = {?USE_DATE?},NEW_VALUE = {?NEW_VALUE?},OLD_VALUE = {?OLD_VALUE?},&#xA;RECORD_TIM=now(),RECORD_MAN='{#sys.username#}',DEPT_COD='{#sys.deptCode#}',USERID='{#sys.userid#}' &#xA;WHERE PUR_NO={?PUR_NO?} AND PARTS_COD = {?PARTS_COD?}" deleteSql="delete from t_waste_apply_detail where  PARTS_COD={?PARTS_COD?} and PUR_NO={?PUR_NO?}"/></body>
