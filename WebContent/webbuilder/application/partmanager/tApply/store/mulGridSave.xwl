	<dbUpdater name="dbUpdater" data="{#detailgrid#}" transaction="commit" insertSql="insert  into T_APPLY_DETAIL( PUR_NO,&#xA;PARTS_COD,&#xA;C_PARTS_NAM,&#xA;E_PARTS_NAM,&#xA;STAND_TXT,&#xA;PUR_TXT,&#xA;PUR_NUM,&#xA;PUR_WAY_COD,&#xA;RECORD_TIM,&#xA;RECORD_MAN,&#xA;DEPT_COD,&#xA;USERID,&#xA;REMARKS,SUPPLIER_NAM,IS_CONTANT,PRICE_VAL,AMOUNT,MIN_UNIT) values ( {?PUR_NO?},&#xA;{?PARTS_COD?},&#xA;{?C_PARTS_NAM?},&#xA;{?E_PARTS_NAM?},&#xA;{?STAND_TXT?},&#xA;{?PUR_TXT?},&#xA;{?PUR_NUM?},&#xA;{?PUR_WAY_COD?},&#xA;now(),'{#sys.username#}','{#sys.deptCode#}','{#sys.userid#}',{?REMARKS?},{?SUPPLIER_NAM?},{?IS_CONTANT?},{?PRICE_VAL?},{?AMOUNT?},{?MIN_UNIT?})" updateSql="update    T_APPLY_DETAIL set &#xA;C_PARTS_NAM={?C_PARTS_NAM?},&#xA;E_PARTS_NAM={?E_PARTS_NAM?},&#xA;STAND_TXT={?STAND_TXT?},&#xA;PUR_TXT={?PUR_TXT?},&#xA;PUR_NUM={?PUR_NUM?},&#xA;PUR_WAY_COD={?PUR_WAY_COD?},&#xA;PRICE_VAL={?PRICE_VAL?},AMOUNT={?AMOUNT?},MIN_UNIT={?MIN_UNIT?},&#xA;REMARKS={?REMARKS?},SUPPLIER_NAM = {?SUPPLIER_NAM?} ,IS_CONTANT = {?IS_CONTANT?} where PARTS_COD={?PARTS_COD?} AND PUR_NO={?PUR_NO?}" deleteSql="delete from T_APPLY_DETAIL where PARTS_COD={?PARTS_COD?} AND PUR_NO={?PUR_NO?}"/></body>
