	<extMenu name="menu"><extLabel name="extLabel811" caption="仓库名称："/><extTextField name="storeNam"/><extLabel name="extLabel17" caption="编号"/><extTextField name="partCod" width="80"/><extLabel name="extLabel1" caption="名称"/><extTextField name="partName" width="80"/><extLabel name="extLabel2" caption="规格"/><extTextField name="standStr" width="80"/><extLabel name="extLabel" caption="日期："/><extDate name="bgTim"/><extLabel name="extLabel13" html="-"/><extDate name="endTim"/><extMenuItem caption="查询" iconCls="icon_find" name="findMenu" onClick="var stName = stCod.getValue();&#xA;if(stName =='')&#xA;{&#xA;	extShowMessage('请选择仓库！');&#xA;	return;&#xA;}&#xA;var beginDate =  Ext.getCmp('bgTim').getValue().format('Y-m-d') ;&#xA;var endDate =  Ext.getCmp('endTim').getValue().format('Y-m-d') ;&#xA;&#xA;//Get('iframe').src='jsp/ireport/inQuery.jsp?begTim='+beginDate+'&amp;endTim='+endDate+'&amp;storeName='+stName ;&#xA;Get('iframe').src='mydesk_inReport.do?begTim='+beginDate+'&amp;endTim='+endDate+'&amp;storeName='+stName ;"/><extTextField name="stCod" hidden="true"/></extMenu><extViewPort name="viewPort" layout="border"><extPanel name="panel1" border="false" region="west" layout="fit" height="100%" split="true" width="150" caption="仓库列表"><extTree name="tree" autoScroll="true" onClick="Ext.getCmp(&quot;storeNam&quot;).setValue(node.text);&#xA;Ext.getCmp(&quot;stCod&quot;).setValue(node.id);&#xA;p=node;" remoteUrl="main?action=webbuilder/application/partmanager/partDevic/WLOutWare/jsonQuery.xwl" onBeforeLoad="control.baseParams.id=node.id;"/></extPanel><extPanel name="panel2" border="false" layout="fit" region="center" split="true" tbar="menu" height="screen.height"><iframe name="iframe" src="jsp/ireport/inQuery.jsp" width="100%" height="100%"/></extPanel></extViewPort><extScript name="extScript" extFooter="var d=new Date();&#xA;Ext.getCmp('endTim').setValue(d.getDate()+5);&#xA;d.setDate(d.getDate()-5);&#xA;Ext.getCmp('bgTim').setValue(d);"/></body>
