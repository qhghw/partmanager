<body name="body" class="sys_normal" createBody="false">
	<query name="savePartsCode" sql="insert  into c_parts_code (PARTS_TYPE,PARTS_COD,C_PARTS_NAM,E_PARTS_NAM,STAND_TXT,STAND_UNIT,&#xA;STAND_CHANGE,MIN_UNIT,STORE_NUM,MIN_NUM,SUPPLIER_NAM,RESP_NAM,RESP_DEPT,REMARKS,RECORD_TIM,&#xA;RECORD_MAN,DEPT_COD,USERID) values({?PARTS_TYPE?},CONCAT('MA-', left(now()+0,8),'-',nextval('PartsCode')),{?C_PARTS_NAM?},{?E_PARTS_NAM?},&#xA;{?STAND_TXT?},{?STAND_UNIT?},{?STAND_CHANGE?},{?MIN_UNIT?},{?STORE_NUM?},{?MIN_NUM?},{?SUPPLIER_NAM?},&#xA;{?RESP_NAM?},{?RESP_DEPT?},{?REMARKS?},now(),'{#sys.username#}','{#sys.deptCode#}','{#sys.userid#}')" type="update"/></body>
